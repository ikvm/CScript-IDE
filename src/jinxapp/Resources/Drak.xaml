<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--ACCENT COLORS-->
    <Color x:Key="HighlightDarkColor">#00000F9E</Color>
    <Color x:Key="HighlightLightColor">#f0f00F9E</Color>

    <Color x:Key="AccentDarkColor">#f0e00F9E</Color>
    
    <!--80%-->
    <Color x:Key="AccentColor">#00020F9E</Color>
    <!--60%-->
    <Color x:Key="AccentColor2">#000e0F9E</Color>
    <!--40%-->
    <Color x:Key="AccentColor3">#f00e0F9E</Color>
    <!--20%-->
    <Color x:Key="AccentColor4">#f0f00F9E</Color>

    <LinearGradientBrush x:Key="BrandingBrush"
                         EndPoint="0.001,0.5"
                         StartPoint="1.002,0.5">
        <GradientStop Color="{DynamicResource HighlightDarkColor}"
                      Offset="0" />
        <GradientStop Color="#f0e00F9E"
                      Offset="1" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="AccentColorBrush"
                     Color="{DynamicResource AccentColor}" />




</ResourceDictionary>